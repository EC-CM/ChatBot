	<!-- ACRONYMS -->
	<category>
		<pattern>What does * mean</pattern>
		<template>
			<srai>
				What does <star/> stand for?
			</srai>
		</template>
	</category>


	<!-- GOODBYE -->

	<category>
		<pattern>Goodbye</pattern>
		<template>
			<srai>Bye</srai>
		</template>
	</category>


	<!-- HOW ARE YOU -->

	<category>
		<pattern>* How are you</pattern>

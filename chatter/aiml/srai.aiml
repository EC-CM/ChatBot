			<srai>How are you</srai>
		</template>
	</category>

	<category>
		<pattern>* How are you *</pattern>
		<template>
			<srai>How are you</srai>
		</template>
	</category>
	
	
	<!-- NAME -->
	
	<category>
		<pattern>* know my name</pattern>

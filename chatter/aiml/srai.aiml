<?xml version="1.0" encoding="iso-8859-1"?>
<aiml>
	<!-- Alternative inputs -->

	<!--HI-->

	<category>
		<pattern>hello</pattern>
		<template>
			<srai>hi</srai>
		</template>
	</category>

	<!-- KEYWORD DETECTION -->
	<category>
		<pattern>hi *</pattern>
		<template>
			<srai>hi</srai>
			<srai>
				<star/>
			</srai>
			<!-- MULTI MESSAGES -->
		</template>
	</category>

